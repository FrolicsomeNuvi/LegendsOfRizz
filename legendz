local StarterGui = game:GetService("StarterGui")
local function showNotification(title, text, duration)
    StarterGui:SetCore("SendNotification", {
        Title = title;
        Text = text;
        Duration = duration; 
    })
end

local function copyToClipboard()
    local textToCopy = "https://discord.gg/4mm4q2NNkV"
    showNotification("Script Moved to New Github!, Join Discord for updates.", "Link copied to your clipboard: " .. textToCopy, 5)
end

copyToClipboard()
